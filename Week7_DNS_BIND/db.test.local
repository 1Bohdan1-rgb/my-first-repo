$ORIGIN test.local.
$TTL 3600
@   IN SOA  ns1.test.local. admin.test.local. (
        2025110101 ; serial
        3600       ; refresh
        900        ; retry
        1209600    ; expire
        300 )      ; minimum

@       IN NS  ns1
ns1     IN A   127.0.0.1
server1 IN A   192.168.1.10
mail    IN A   192.168.1.20
www     IN CNAME server1
@       IN MX 10 mail
@       IN TXT "Learning DNS Week7"
